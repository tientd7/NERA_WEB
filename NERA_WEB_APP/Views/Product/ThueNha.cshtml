@{
    ViewBag.Title = "Quản lý khách hàng thuê";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<section class="container">
    <h3 class="title-page">Danh sách</h3>

    <form>
        <div ng-controller="ProductCtrl">
            <table class="table-wrapper table-bordered table-hover container">
                <tr class="title-name-column">

                    <th>Họ tên</th>
                    <th>Số điện thoại</th>
                    <th>Post_Id</th>
                    <th>Tên Bài Viết</th>
                    <th>Id Sản phẩm</th>
                    <th>Ngày Đăng Ký</th>
                    <th>Chưa Duyệt</th>
                    <th>Ngày Đọc</th>
                <tr dir-paginate="item in ListKh | itemsPerPage:5">
                    <td>{{item.cus_name}}</td>
                    <td>{{item.cus_phone}}</td>
                    <td>{{item.Post_Id}}</td>
                    <td>{{item.Post_Title}}</td>
                    <td>{{item.Item_Id}}</td>
                    <td>{{item.create_date |mydate| date: 'dd-MM-yyyy'}}</td>
                    <td>{{item.unread}}</td>
                    <td>{{item.read_date |mydate| date: 'dd-MM-yyyy'}}</td>
                </tr>
            </table>


        </div>
    </form>
</section>


@section JavaScript{
    <script src="~/Scripts/Controller/Product/product.js"></script>

}




