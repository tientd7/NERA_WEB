                @Html.Partial("~/Views/Shared/pages/dialog_popup/dialog_signIn.cshtml")
                @Html.Partial("~/Views/Admin/header.cshtml")
                
                @RenderBody()
      
                @Html.Partial("~/Views/Shared/pages/footer/footer.cshtml")
            </div>
        </div>


        <!-- OFFCANVAS MENU -->
        @Html.Partial("~/Views/Shared/pages/menu_off_canvas/menu_off_canvas.cshtml")
        <!-- Preloader -->
        @Html.Partial("~/Views/Shared/pages/preloader/reload.cshtml")
        @Html.Partial("~/Views/Shared/pages/preloader/preloader.cshtml")



    </div>

