@{
    ViewBag.Title = "Home";
}
<div id="slide-boxsearch" style="position:relative; top:-30px;">
    <div id="main-carousel" class="carousel slide hero-slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            @{ var i = 0; }
            @foreach (var slide in ViewBag.slideImageTop)
            {
                i++;
                if (slide.Image_Order == 1)
                {
                    <li data-target="#main-carousel" data-slide-to="@i" class="active"></li>
                }
                else
                {
                    <li data-target="#main-carousel" data-slide-to="@i"></li>
                }
            }
        </ol>
        <!-- Wrapper for slides -->

        <div class="carousel-inner slide-img" role="listbox">

            @foreach (var slide in ViewBag.slideImageTop)
            {
                if (slide.Image_Order == 1)
                {
                    <div class="item active">

                        <img src="@slide.Image_URL" />

                        <!--Slide Image-->

                        <div class="container">
                            <div class="carousel-caption">
                                <small class="animated fadeIn">Gọi cho chúng tôi</small>
                                <div class="phone animated lightSpeedIn">+84 988.888.888</div>
                                <h1 class="animated lightSpeedIn">
                                    <a href="@slide.Image_Link" style="color: #f50707;">
                                        @slide.Image_Title
                                    </a>
                                </h1>

                                <p class="lead animated lightSpeedIn">Thông tin về dự án</p>

                                <a class="btn btn-primary  animated lightSpeedIn" href="#">làm việc với chúng tôi</a>
                            </div>
                            <!--.carousel-caption-->
                        </div>
                        <!--.container-->
                    </div>
                }
                else
                {
                    <div class="item">
                        <img src="@slide.Image_URL" />

                        <!--Slide Image-->

                        <div class="container">
                            <div class="carousel-caption">
                                <small class="animated fadeIn">Gọi cho chúng tôi</small>
                                <div class="phone animated lightSpeedIn">+84 988.888.888</div>
                                <h1 class="animated lightSpeedIn">
                                    <a href="@slide.Image_Link" style="color: #f50707;">
                                        @slide.Image_Title
                                    </a>
                                </h1>

                                <p class="lead animated lightSpeedIn">Thông tin về dự án</p>

                                <a class="btn btn-primary animated lightSpeedIn" href="#">làm việc với chúng tôi</a>
                            </div>
                            <!--.carousel-caption-->
                        </div>
                        <!--.container-->
                    </div>
                }
            }

            <!--.item-->
            <!--.item-->
        </div>
        <!--.carousel-inner-->
        <!-- Controls -->
        <a class="left carousel-control" href="#main-carousel" role="button" data-slide="prev">
            <i class="fa fa-angle-left" aria-hidden="true"></i>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#main-carousel" role="button" data-slide="next">
            <i class="fa fa-angle-right" aria-hidden="true"></i>
            <span class="sr-only">Next</span>
        </a>
    </div>
