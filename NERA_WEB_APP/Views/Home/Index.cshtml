<section class="testimonial-wrap">

    <div id="slideshow-maps">
        <div id="slideshow-fb">

            <div id="myCarousel" class="carousel slide" data-ride="carousel">

                <ol class="carousel-indicators">
                    @{ var j = 0; }
                    @foreach (var slide in ViewBag.slideImagefeedback)
                    {
                        j++;
                        if (slide.Image_Order == 1)
                        {
