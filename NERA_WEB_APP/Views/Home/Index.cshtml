<!-- Testimonial-wrap start -->
<section class="testimonial-wrap">

    <div id="slideshow-maps">
        <div id="slideshow-fb">

            <div id="myCarousel" class="carousel slide" data-ride="carousel">

                <ol class="carousel-indicators">
                    @{ var j = 0; }
                    @foreach (var slide in ViewBag.slideImagefeedback)
                    {
                        j++;
                        if (slide.Image_Order == 1)
                        {
                            <li data-target="#myCarousel" data-slide-to="@j" class="active"></li>
                        }
                        else
                        {
                            <li data-target="#myCarousel" data-slide-to="@j"></li>
                        }
                    }
                    @*<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>*@
